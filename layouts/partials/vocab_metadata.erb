<ul class="none">
  <% vocabularies_for(group).each do |vocab| %>
    <li>
      <dl resource="<%= h vocab.fetch(:vocab_uri) %>" property="void:vocabulary" typeof="voaf:Vocabulary">
        <dt><a property="vann:preferredNamespaceUri" content="<%= h vocab.fetch(:namespace_uri) %>" href="<%= h vocab.fetch(:vocab_uri) %>"><abbr property="vann:preferredNamespacePrefix"><%= h vocab.fetch(:prefix) %></abbr></a>: <span property="dc:title"><%= h vocab.fetch(:title) %></span></dt>
        <dd property="dc:description"><%= h vocab.fetch(:description) %></dd>
      </dl>
    </li>
  <% end %>
</ul>
